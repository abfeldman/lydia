#! /bin/sh

# Remove all files and directories that are generated by the build system

# Generated files
rm -f aclocal.m4 \
      config.log \
      config.status \
      configure \
      stamp-h1 \
      include/config.h \
      include/config.h.in

# Generated directories
rm -rf autom4te.cache \
       auxdir \
       `find . -name '.deps'` \
       `find . -name Makefile.in` \
       `find . -name Makefile`

