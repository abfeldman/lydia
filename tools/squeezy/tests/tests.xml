<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE testsuite SYSTEM "../../testsuite/lydia_tests.dtd">

<testsuite>
  <separator>Arithmetic Circuits:</separator>
  <test>
    <title>1-bit full adder (hierarchical CNF)</title>
    <input>2adder-strong.hcnf</input>
    <output>2adder-strong.hcnf.sq</output>
  </test>
  <test>
    <title>1-bit full adder (hierarchical DNF)</title>
    <input>2adder-strong.hdnf</input>
    <output>2adder-strong.hdnf.sq</output>
  </test>
  <test>
    <title>1-bit full adder (flat CNF)</title>
    <input>2adder-strong.cnf</input>
    <output>2adder-strong.cnf.sq</output>
  </test>
  <test>
    <title>1-bit full adder (flat DNF)</title>
    <input>2adder-strong.dnf</input>
    <output>2adder-strong.dnf.sq</output>
  </test>
</testsuite>
